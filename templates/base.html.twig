<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    <link rel="icon"
        href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
    <script src="https://kit.fontawesome.com/37d81ace64.js" crossorigin="anonymous"></script>
    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
    {{ encore_entry_link_tags('style') }}
    {% endblock %}

    {% block javascripts %}
    {{ encore_entry_script_tags('script') }}
    {% endblock %}
</head>
<body>
<header class="">
    <div class="container-fluid bg-primary p-2">
        <div class="row">
            <nav class="navbar navbar-expand-lg navbar-dark text-light ">
                <div class="text-center">
                    <a href="/"><img src="{{ asset('build/images/RlogoAnimalXfr.png') }}" class='img-fluid' alt="Logo"></a>
                    <p class="mt-2">Vers l'infini et au delà</p>
                </div>
                <div class="row">
                    <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                        data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <div class="text-center">
                        <ul class="navbar-nav ms-5">
                            <li class="nav-item">
                                <a class="nav-link" href="#"><img src="{{ asset('build/images/chien_emot_w.png') }}" class='img-fluid emot' alt="Logo"></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#"><img src="{{ asset('build/images/chat_emot_w.png') }}" class='img-fluid emot' alt="Logo"></a>
                            </li>
                             <li class="nav-item">
                                <a class="nav-link" href="#"><img src="{{ asset('build/images/oiseau_emot_w.png') }}" class='img-fluid emot' alt="Logo"></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#"><img src="{{ asset('build/images/poisson_emot_w.png') }}" class='img-fluid emot' alt="Logo"></a>
                            </li>
                             <li class="nav-item">
                                <a class="nav-link" href="#"><img src="{{ asset('build/images/ratw_emot_w.png') }}" class='img-fluid emot' alt="Logo"></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#"><img src="{{ asset('build/images/gecko_emot_w.png') }}" class='img-fluid emot' alt="Logo"></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#"><img src="{{ asset('build/images/poule_emot_w.png') }}" class='img-fluid emot' alt="Logo"></a>
                            </li>
                        </ul>
                        <ul class="navbar-nav ms-5">
                            <li class="nav-item">
                                <a class="nav-link" href="#"><i class="fa-solid fa-envelope-circle-check fa-2xl"></i></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#"><i class="fa-solid fa-piggy-bank fa-2xl"></i></i></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="#"><i class="fa-solid fa-basket-shopping fa-2xl"></i></a>
                            </li>
                        </ul>
                    </div>
                        <form class="d-flex">
                            <input class="form-control me-2" type="search" placeholder="Rechercher" aria-label="Search">
                            <button class="btn btn-outline-light" type="submit"><i
                                    class="fa-solid fa-magnifying-glass"></i>
                            </button>
                        </form>
                    </div>
                </div>
            </nav>
        </div>
    </div>
</header>

<section>
    {% block body %}{% endblock %}
</footer>


<footer>

</footer>



</body>

</html>